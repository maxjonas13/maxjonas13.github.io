{
  "projectName": "Rodenbach",
  "framework": "kirby",
  "vhost": "myblog.tdlinx.int",
  "browserSupport": [
    "ie >= 11",
    "last 1 Firefox version",
    "last 1 Chrome version",
    "last 1 Safari version",
    "last 1 Opera version"
  ],
  "directories": {
    "clean": [
      "../assets/graphics",
      "../assets/scripts",
      "../assets/styles",
      "../assets/symbols"
    ],
    "cwd": "../src",
    "dest": {
      "default": "../assets"
    }
  },
  "dirNames": {
    "resources": "../assets"
  }
}